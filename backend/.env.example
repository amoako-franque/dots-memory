# Server Configuration
PORT=30700
NODE_ENV=development
API_URL=http://localhost:30700
FRONTEND_URL=http://localhost:5173

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/dots_memory?schema=public

# Redis
REDIS_URL=redis://localhost:6379

# JWT Authentication
JWT_SECRET=your-jwt-secret-change-in-production
JWT_REFRESH_SECRET=your-refresh-secret-change-in-production

# AWS S3 Configuration
S3_ENABLED=false
S3_REGION=us-east-1
S3_BUCKET=your-bucket-name
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
S3_ENDPOINT=
S3_FORCE_PATH_STYLE=false
S3_CDN_URL=
S3_MAX_FILE_SIZE_MB=100
S3_ALLOWED_MIME_TYPES=image/jpeg,image/png,image/gif,image/webp,image/svg+xml,video/mp4,video/webm,video/quicktime,video/x-msvideo
S3_ALLOWED_EXTENSIONS=.jpg,.jpeg,.png,.gif,.webp,.svg,.mp4,.webm,.mov,.avi
S3_PRESIGNED_URL_EXPIRATION=3600
S3_ENCRYPTION_ENABLED=true
S3_ENCRYPTION_ALGORITHM=AES256
S3_CORS_ORIGINS=*
S3_CORS_METHODS=GET,PUT,POST,DELETE,HEAD
S3_CORS_HEADERS=*
S3_CORS_MAX_AGE=3600

# Cloudinary Configuration
CLOUDINARY_ENABLED=false
CLOUDINARY_CLOUD_NAME=your-cloud-name
CLOUDINARY_API_KEY=your-api-key
CLOUDINARY_API_SECRET=your-api-secret
CLOUDINARY_SECURE=true
CLOUDINARY_FOLDER=memory
CLOUDINARY_MAX_FILE_SIZE_MB=100
CLOUDINARY_ALLOWED_MIME_TYPES=image/jpeg,image/png,image/gif,image/webp,image/svg+xml,video/mp4,video/webm,video/quicktime,video/x-msvideo
CLOUDINARY_ALLOWED_EXTENSIONS=.jpg,.jpeg,.png,.gif,.webp,.svg,.mp4,.webm,.mov,.avi
CLOUDINARY_IMAGE_QUALITY=auto
CLOUDINARY_IMAGE_FORMAT=auto
CLOUDINARY_IMAGE_WIDTH=
CLOUDINARY_IMAGE_HEIGHT=
CLOUDINARY_VIDEO_QUALITY=auto
CLOUDINARY_VIDEO_FORMAT=auto
CLOUDINARY_UPLOAD_PRESET=
CLOUDINARY_SIGNATURE_ALGORITHM=sha1

# Logging
LOG_LEVEL=debug

# Payment Providers
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
PAYSTACK_SECRET_KEY=
PAYSTACK_PUBLIC_KEY=
